{
    "sap.app": {
		"id": "tips.mes",
        "i18n": "i18n/i18n.properties",
        "i18nCommon": "../common/i18n/i18n.properties",
        "title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
    },
    "sap.ui5": {
		"rootView": {
			"viewName": "tips.mes.view.frame.Index",
			"type": "XML",
			"async": true
		},
        "dependencies": {
        	"libs": {
        		"sap.ui.layout": {},
                "sap.m": {}
            }
        }, 
        "resources": {
        	"css" : [
	        	{ "id": "css",		"uri": "css/style.css" 			},
	        	{ "id": "comCss",	"uri": "../common/css/style.css" 	}
	        ]
        },
        "models": {
        	"i18n": {
        		"type": "sap.ui.model.resource.ResourceModel",
                "settings": {
					"bundleName": "tips.mes.i18n.i18n"
				}
            },
            "i18nCommon": {
        		"type": "sap.ui.model.resource.ResourceModel",
                "settings": {
					"bundleName": "tips.common.i18n.i18n"
				}
            }
        },
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"controlId": "rootControl",
				"viewPath": "tips.mes.view.contents",
				"controlAggregation": "pages",
				"transition": "show",
				"bypassed": {
					"target": [
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{ "name": "main", "pattern": "", "target": "main" },
				{ "name": "detail3", "pattern": "orderMgmt_detail", "target": "detail3"}
			],
			"targets": {
				"main": { "viewId": "main", "viewName": "Main", "viewLevel": 1 },
				"notFound":  { "viewPath": "tips.common.view", "viewName": "NotFound" },
				"detail3": {"viewPath": "tips.mes.view.contents.orderMgmt.submenu","viewName": "orderMgmt_detail","viewLevel": 1,"viewId": "orderMgmt_detail"}
			}
		}
    }
}